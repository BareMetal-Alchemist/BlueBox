<!DOCTYPE html>
<html>
<head>
  <title><%= title %></title>
  <link rel="stylesheet" href="/style.css">
</head>
<body>
  <h1><%= title %></h1>
  <table>
    <thead>
      <tr><th>Machine</th><th>User</th><th>Password</th></tr>
    </thead>
    <tbody>
      <% data.forEach(entry => { %>
        <tr>
          <td><%= entry.machine %></td>
          <td><%= entry.user %></td>
          <td><%= entry.password %></td>
        </tr>
      <% }) %>
    </tbody>
  </table>
</body>
</html>
